{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_id": { "type": "string" },
          "productName": { "type": "string" },
          "productCategory": { "type": "string" },
          "productSubCategory": { "type": "string" },
          "productPrice": { "type": "integer" },
          "productDescription": { "type": "string" },
          "productImage": { "type": "string", "format": "uri" },
          "productRating": { "type": "string" },
          "productTotalOrders": { "type": "string" },
          "productStatus": { "type": "boolean" },
          "productFor": { "type": "string" },
          "productAddedBy": { "type": "string" },
          "__v": { "type": "integer" }
        },
        "required": ["_id", "productName", "productCategory", "productPrice"]
      }
    },
    "count": { "type": "integer" },
    "message": { "type": "string" }
  },
  "required": ["data", "count", "message"]
}
